             xmlns:local="clr-namespace:CryptoTask"
             StartupUri="Views/MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Dictionaries/LightDictionary.xaml"/>
				<ResourceDictionary Source="Dictionaries/DarkDictionary.xaml"/>
				<ResourceDictionary Source="Dictionaries/Buttons.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
